	"tokenColors": [
		{
			"settings": {
				"background": "#1d1d1d",
				"foreground": "#ccc"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#6a6",
				"fontStyle": ""
			}
		},
		{
			"name": "Normal",
			"scope": [
				"variable",
				"entity.name.function",
				"meta.function",
				"meta.function-call",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#ccc",
				"fontStyle": ""
			}
		},
		{
			"name": "Darker",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.other",
				"storage.type",
				"storage.modifier",
				"support.type.sys-types",
				"support.type.posix-reserved",
				"support.type.stdint",
				"constant.language",
				"keyword.operator",
				"keyword.operator.logical",
				"keyword.operator.arithmetic",
				"keyword.operator.assignment",
				"keyword.operator.new",
				"punctuation.separator",
				"support.variable.glsl"
			],
			"settings": {
				"foreground": "#999",
				"fontStyle": ""
			}
		},
		{
			"name": "Purple",
			"scope": [
				"string",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#aad",
				"fontStyle": ""
			}
		},
		{
			"name": "Dark Purple",
			"scope": [
				"meta.preprocessor",
				"meta.preprocessor.include",
				"meta.preprocessor.macro",
				"keyword.control.directive.conditional",
				"keyword.control.directive.include",
				"keyword.control.directive.define",
				"keyword.control.directive.undef",
				"constant.character.escape.line-continuation",
				"keyword.control.directive.pragma",
				"meta.preprocessor.pragma",
				"keyword.control.hlsl",
				"markup.heading"
			],
			"settings": {
				"foreground": "#77a",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#E32",
				"fontStyle": ""
			}
		},
		{
			"name": "Makrup Plaintext",
			"scope": [
				"meta.paragraph.markdown"
			],
			"settings": {
				"foreground": "#ccc",
				"fontStyle": ""
			}
		},
		{
			"name": "Markup Bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"foreground": "#aaa",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup Italics",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#aaa",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Makeup Code",
			"scope": [
				"markup.inline.raw",
				"markup.raw"
			],
			"settings": {
				"foreground": "#aad",
				"fontStyle": ""
			}
		},
		{
			"name": "Markup List",
			"scope": [
				"markup.list",
				"beginning.punctuation.definition.list.markdown"
			],
			"settings": {
				"foreground": "#aaa",
				"fontStyle": ""
			}
		}
	]
}